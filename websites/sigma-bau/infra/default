    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/mannseicher.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/mannseicher.com/privkey.pem; # managed by Certbot
