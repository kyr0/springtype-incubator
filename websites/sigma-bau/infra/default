server {
    if ($host = mad2man.de) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = digi-hood.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = digi-hood.de) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = mannseicher.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


	listen 80 ;
	listen [::]:80 ;
    server_name mannseicher.com digi-hood.de digi-hood.com mad2man.de;
    return 404; # managed by Certbot








}
